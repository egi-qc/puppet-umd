{
  "name": "puppet-umd",
  "version": "0.1.0",
  "author": "Pablo Orviz",
  "summary": "UMD Puppet module",
  "license": "Apache-2.0",
  "source": "git://github.com/egi-qc/puppet-umd.git",
  "project_page": "https://github.com/egi-qc/puppet-umd",
  "issues_url": "https://github.com/egi-qc/puppet-umd/issues",
  "requirements": [
    { "name": "puppet","version_requirement": "3.x" }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["5", "6", "7"]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["14.04"]
    }
  ],
  "dependencies": [
    { "name": "puppetlabs/stdlib", "version_requirement": ">=4.0.0 <5.0.0" },
    { "name": "puppetlabs/apt", "version_requirement": ">=2.3.0" }
  ]
}
